{
  "extends": "./../tsconfig.json",
  "compilerOptions": {
    "target": "es2016",
    "outDir": "./_tests",
    "sourceMap": false,
    "strict": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": false,
    "noImplicitThis": false,
    "allowJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "declaration": false,
    "declarationDir": null
  },
  "include": [
    "./../preDownloadMultipleSecureFiles.ts",
    "./**/*.ts"
  ],
  "exclude": ["node_modules"]
}
